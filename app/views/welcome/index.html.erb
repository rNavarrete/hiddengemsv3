  <div class="row col-md-12">
    <div class= "col-md-2">  
      <h4>Most Popular</h4>
      <h4>Recent</h4>
      <h4>Most Popular</h4>
      <h4>Most Popular</h4>
      <h4>Most Popular</h4>
      <h4>Most Popular</h4>
    </div>   

    <div class="col-md-10">
      <ul class="thumbnails list-unstyled">
        <% @videos.each do |video| %>
          <div class="item">
            <li class="col-md-4">
              <div class="thumbnail img-polaroid"> 
                <%= raw youtube_embed(video.video_link)%>
                <div class="caption well-large">  
                  <span><%= "Artist: #{video.artist}" %></span><br>
                  <%= "Song: #{video.song}"%><br>
                  <%= "Found by: #{video.submitted_by}"%><br></span>
                   <%= button_to "Gem!", video_vote_up_path(video.id), class: "btn btn-info navbar-btn" %>
                  <div class="modal-footer" style="text-align: left; border-style:outset;">
                    <div class="progress progress-striped active" style="background: #ddd; border-style:outset;">
                      <div class="bar" style="width: 40%"></div>
                    </div>
                  </div>  
                </div>    
              </div>             
            </li>
          </div>  
        <% end %>
      </ul>
    </div>
  </div>            


